      <select>var dbConn;
var result;
var destinationpath=&quot;c:/dev_chv/out&quot;;

function ecrire_fichier( nomfichier, lignes)
{
FileUtil.write(destinationpath+&apos;/&apos;+nomfichier, false, lignes);
}

try {
	dbConn = DatabaseConnectionFactory.createDatabaseConnection(&apos;org.postgresql.Driver&apos;,&apos;jdbc:postgresql://localhost:5432/postgres&apos;,&apos;postgres&apos;,&apos;admin&apos;);

	result = dbConn.executeCachedQuery(&quot;SELECT c1,to_char(c2,&apos;YYYYMMDD&apos;) as \&quot;c2\&quot;,c3,c4,c4,c5,c6,c7,c8,c9,c10,c11,c12,c13,c14,c15,c16,c17,filename FROM ordsage WHERE statut=&apos;1&apos; ORDER by filename, c1, c4, c2 asc&quot;);


//traitement du résultat...:
var lignes=&quot;&quot;;
var fichier=&quot;&quot;;
var currfichier=&quot;&quot;;


while (result.next())
{

     //lecture du nom du fichier
     fichier=result.getString(&apos;filename&apos;);
     
	//traiter selon le nom du fichier...
	if (currfichier==&apos;&apos;) 
	{currfichier=fichier;}
	else 
	{
		if (currfichier!=fichier)
		{
			//detection changement de fichier
			//ecrire les lignes dans le fichier
			if (lignes!=&apos;&apos;){ecrire_fichier(currfichier,lignes)}
			//remplacer par le nouveau nom...
			currfichier=fichier;
			lignes=&apos;&apos;; //reset lignes
		}
	}

	//lire les données
	lignes=lignes+result.getString(&apos;c1&apos;)+&quot;;&quot;+result.getString(&apos;c2&apos;)+&quot;;&quot;+result.getString(&apos;c3&apos;)+&quot;;&quot;+result.getString(&apos;c4&apos;)+&quot;;&quot;+result.getString(&apos;c5&apos;)+&quot;;&quot;+result.getString(&apos;c6&apos;)+&quot;;&quot;+result.getString(&apos;c7&apos;)+&quot;;&quot;+result.getString(&apos;c8&apos;)+&quot;;&quot;+result.getString(&apos;c9&apos;)+&quot;;&quot;+result.getString(&apos;c10&apos;)+&quot;;&quot;+result.getString(&apos;c11&apos;)+&quot;;&quot;+result.getString(&apos;c12&apos;)+&quot;;&quot;+result.getString(&apos;c13&apos;)+&quot;;&quot;+result.getString(&apos;c14&apos;)+&quot;;&quot;+result.getString(&apos;c15&apos;)+&quot;;&quot;+result.getString(&apos;c16&apos;)+&quot;;&quot;+result.getString(&apos;c17&apos;)+&quot;\n&quot;;
	
}

if (lignes!=&apos;&apos;){ecrire_fichier(fichier,lignes)}

} catch(e) {logger.info(&quot;Pas de données a extraire...&quot;);}
finally {
	if (dbConn) { 
		dbConn.close();
	}
}

return result;</select>
